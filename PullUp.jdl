entity UserProfile {
  username String
  email String
  password String
  longitude String
  latitude String
}

entity BasketballCourt {
  courtName String
  state String
  zipCode Integer
  streetAddress String
  longitude String
  latitude String
  courtTypeID Integer
}

entity CourtType {
  typeName String
}

relationship OneToMany {
  CourtType{courtTypeID} to BasketballCourt{courtType}
}

relationship ManyToMany {
  UserProfile{savedCourts} to BasketballCourt{userProfiles}
}


